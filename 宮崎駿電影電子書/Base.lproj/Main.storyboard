                            <textView clipsSubviews="YES" multipleTouchEnabled="YES" contentMode="scaleToFill" fixedFrame="YES" textAlignment="natural" translatesAutoresizingMaskIntoConstraints="NO" id="U0a-ei-jIH">
                                <rect key="frame" x="64" y="430" width="286" height="398"/>
